{
  "kind": "collectionType",
  "collectionName": "properties",
  "info": {
    "singularName": "property",
    "pluralName": "properties",
    "displayName": "Property"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "location": {
      "type": "string"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "apartment",
        "house",
        "land",
        "studio",
        "warehouse"
      ]
    },
    "purpose": {
      "type": "enumeration",
      "enum": [
        "rent",
        "sale"
      ]
    },
    "area": {
      "type": "biginteger"
    },
    "baths": {
      "type": "integer"
    },
    "rooms": {
      "type": "integer"
    },
    "description": {
      "type": "richtext"
    },
    "price": {
      "type": "float"
    },
    "furnishingStatus": {
      "type": "boolean"
    },
    "photos": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "available",
        "sold"
      ]
    }
  }
}
